    "tslib": "2.3.1",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.1",
    "@angular-devkit/core": "16.2.1",
    "@angular-devkit/schematics": "16.2.1",
